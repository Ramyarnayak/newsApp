class FavouritesPage {
    constructor(newsService, storageService, networkService, loadingCtrl, popoverCtrl) {
        this.newsService = newsService;
        this.storageService = storageService;
        this.networkService = networkService;
        this.loadingCtrl = loadingCtrl;
        this.popoverCtrl = popoverCtrl;
        this.sliderOptions = {
            allowSlidePrev: false,
            allowSlideNext: false,
        };
    }
    presentPopover(event) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const popover = yield this.popoverCtrl.create({
                component: _favourites_popover_favourites_popover__WEBPACK_IMPORTED_MODULE_2__["PopoverPage"],
                event: event,
            });
            yield popover.present();
        });
    }
    ngOnInit() { }
    // get news detail via news API service
    onGoToNewsDetail(article) {
        this.newsService.getNewsDetail(article);
    }
    // refresh page via network service
    onRefresh(event) {
        this.networkService.refreshPage(event);
    }
    onRemoveFavourite(article, slidingItem) {
        slidingItem.close();
        this.loadingCtrl
            .create({
            message: "Deleting...",
        })
            .then((loadingEl) => {
            loadingEl.present();
            this.storageService.removeFromFavourites(article);
            loadingEl.dismiss();
        });
    }
    onClearAll() { }
}
